{
  "name": "@swingletree-oss/scotty-client",
  "description": "Scotty Client",
  "repository": {
    "type": "git",
    "url": "git@github.com:swingletree-oss/scotty-client.git"
  },
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "npm run build && node bin/main.js",
    "build": "npm run build-ts && npm run tslint",
    "test": "ts-mocha --exit",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "watch-debug": "nodemon --inspect bin/main.js",
    "release": "semantic-release"
  },
  "dependencies": {
    "@swingletree-oss/harness": "^2.0.2",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/github": "^7.0.7",
    "@semantic-release/npm": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/chai": "^4.2.12",
    "@types/chai-things": "0.0.34",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.27",
    "@types/request": "^2.48.5",
    "@types/sinon": "^9.0.4",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-things": "^0.2.0",
    "eslint": "^7.7.0",
    "inversify": "^5.0.1",
    "mocha": "^8.1.1",
    "mockserver": "^3.1.1",
    "nodemon": "^2.0.4",
    "npm-check-updates": "^7.1.1",
    "semantic-release": "^17.1.1",
    "sinon": "^9.0.3",
    "sinon-chai": "^3.5.0",
    "sinon-express-mock": "^2.2.1",
    "ts-mocha": "^7.0.0",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
  },
  "license": "GPL-3.0",
  "private": false,
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}
